			<file src="bin\Release\net35\*.*" target="lib\net35" />
			<file src="bin\Release\net45\*.*" target="lib\net45" />
			<file src="bin\Release\net472\*.*" target="lib\net472" />
			<file src="bin\Release\netstandard2.0\*.*" target="lib\netstandard2.0" />
